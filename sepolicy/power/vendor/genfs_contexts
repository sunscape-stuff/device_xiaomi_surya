# DT2W
genfscon sysfs /touchpanel u:object_r:sysfs_touchpanel:s0
# CPU frequency management
genfscon sysfs /devices/system/cpu/cpu0/cpufreq/scaling_min_freq u:object_r:sysfs_cpuman:s0
genfscon sysfs /devices/system/cpu/cpu0/cpufreq/scaling_max_freq u:object_r:sysfs_cpuman:s0
genfscon sysfs /devices/system/cpu/cpu6/cpufreq/scaling_min_freq u:object_r:sysfs_cpuman:s0
genfscon sysfs /devices/system/cpu/cpu6/cpufreq/scaling_max_freq u:object_r:sysfs_cpuman:s0
# Kernelspace battery saver
genfscon sysfs /module/battery_saver/parameters/enabled u:object_r:sysfs_battery_saver:s0
